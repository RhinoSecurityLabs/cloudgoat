{
    "Version": "2012-10-17",
    "Statement": {
        "Effect": "Allow",
        "Action": "iam:Get*",
        "Resource": "*",
        "Condition": {
            "DateGreaterThan": {
                "aws:CurrentTime": "2017-07-01T00:00:00Z"
            },
            "DateLessThan": {
                "aws:CurrentTime": "2017-12-31T23:59:59Z"
            }
        }
    }
}